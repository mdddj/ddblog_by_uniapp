<import-sjs name="handler" from="./handler.sjs"></import-sjs>
<view class="interlayer"><block a:for="{{nodes}}" a:for-item="item" a:for-index="index" a:key="index"><block><block a:if="{{item.name=='img'}}"><rich-text class="_img" style="{{('text-indent:0;'+handler.getStyle(item.attrs.style,'inline-block'))}}" id="{{item.attrs.id}}" nodes="{{handler.getNode(item,imgLoad)}}" data-attrs="{{item.attrs}}" data-event-opts="{{[['tap',[['imgtap',['$event']]]],['longTap',[['imglongtap',['$event']]]]]}}" onTap="__e" onLongTap="__e"></rich-text></block><block a:else><block a:if="{{item.type=='text'}}"><text decode>{{item.text}}</text></block><block a:else><block a:if="{{item.name=='br'}}"><text>\n</text></block><block a:else><block a:if="{{item.name=='video'}}"><view><block a:if="{{item.lazyLoad&&!controls[item.attrs.id].play}}"><view class="{{('_video '+(item.attrs.class||''))}}" style="{{(item.attrs.style||'')}}" id="{{item.attrs.id}}" data-event-opts="{{[['tap',[['_loadVideo',['$event']]]]]}}" onTap="__e"></view></block><block a:else><video class="{{(item.attrs.class)}}" style="{{(item.attrs.style||'')}}" id="{{item.attrs.id}}" autoplay="{{item.attrs.autoplay||controls[item.attrs.id]&&controls[item.attrs.id].play}}" controls="{{item.attrs.controls}}" loop="{{item.attrs.loop}}" poster="{{item.attrs.poster}}" src="{{controls[item.attrs.id]?item.attrs.source[controls[item.attrs.id].index]:item.attrs.src}}" unit-id="{{item.attrs['unit-id']}}" data-source="{{item.attrs.source}}" data-from="video" data-event-opts="{{[['play',[['play',['$event']]]],['error',[['error',['$event']]]]]}}" muted="{{item.attrs.muted}}" onPlay="__e" onError="__e"></video></block></view></block><block a:else><block a:if="{{item.name=='audio'}}"><audio class="{{(item.attrs.class)}}" style="{{(item.attrs.style||'')}}" id="{{item.attrs.id}}" author="{{item.attrs.author}}" controls="{{item.attrs.controls}}" loop="{{item.attrs.loop}}" name="{{item.attrs.name}}" poster="{{item.attrs.poster}}" src="{{controls[item.attrs.id]?item.attrs.source[controls[item.attrs.id].index]:item.attrs.src}}" data-source="{{item.attrs.source}}" data-audio="audio" data-event-opts="{{[['error',[['error',['$event']]]]]}}" onError="__e"></audio></block><block a:else><block a:if="{{item.name=='a'}}"><view class="{{('_a '+(item.attrs.class||''))}}" style="{{(item.attrs.style||'')}}" hover-class="navigator-hover" data-attrs="{{item.attrs}}" data-event-opts="{{[['tap',[['linkpress',['$event']]]]]}}" onTap="__e"><trees vue-id="{{'fddf6f7a-1-'+index}}" nodes="{{item.children}}" onVueInit="__l"></trees></view></block><block a:else><block a:if="{{item.name=='li'}}"><view class="{{((item.attrs.class||'')+' _li')}}" style="{{((item.attrs.style||'')+';display:flex')}}"><block a:if="{{item.type=='ol'}}"><view class="_ol-before">{{item.num}}</view></block><block a:else><view class="_ul-before"><block a:if="{{item.floor%3==0}}"><view class="_ul-type1">█</view></block><block a:else><block a:if="{{item.floor%3==2}}"><view class="_ul-type2"></view></block><block a:else><view class="_ul-type1" style="border-radius:50%;">█</view></block></block></view></block><view class="_li-after"><trees vue-id="{{'fddf6f7a-2-'+index}}" nodes="{{item.children}}" onVueInit="__l"></trees></view></view></block><block a:else><block a:if="{{handler.useRichText(item)}}"><rich-text class="{{('__'+item.name)}}" style="{{(''+handler.getStyle(item.attrs.style,'block'))}}" id="{{item.attrs.id}}" nodes="{{[item]}}"></rich-text></block><block a:else><view class="{{('_'+item.name+' '+(item.attrs.class||''))}}" style="{{(item.attrs.style||'')}}" id="{{item.attrs.id}}"><trees vue-id="{{'fddf6f7a-3-'+index}}" nodes="{{item.children}}" onVueInit="__l"></trees></view></block></block></block></block></block></block></block></block></block></block></view>