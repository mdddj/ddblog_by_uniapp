<view><cu-custom vue-id="519f2a76-1" isBack="{{false}}" onVueInit="__l"><view slot="backText">返回</view><view slot="content">归档</view><view slot="right"><view class="action"><view class="{{(('cu-load load-cuIcon')+' '+(isLoad?'loading':'over'))}}"></view></view></view></cu-custom><scroll-view class="nav text-center" scroll-x="{{true}}"><view class="{{(('cu-item')+' '+(0==TabCur?'cur text-purple':''))}}" data-id="0" data-event-opts="{{[['tap',[['tabSelect',['$event']]]]]}}" onTap="__e">按分类</view><view class="{{(('cu-item')+' '+(1==TabCur?'cur  text-purple':''))}}" data-id="1" data-event-opts="{{[['tap',[['tabSelect',['$event']]]]]}}" onTap="__e">按时间</view><view class="{{(('cu-item')+' '+(2==TabCur?'cur  text-purple':''))}}" data-id="2" data-event-opts="{{[['tap',[['tabSelect',['$event']]]]]}}" onTap="__e">按标签</view></scroll-view><block a:if="{{TabCur==0}}"><view class="cu-list menu"><block a:for="{{categorys}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{item.size!==0}}"><view data-event-opts="{{[['tap',[['toList',['cate','$0','$1'],[[['categorys','',index,'id']],[['categorys','',index,'name']]]]]]]}}" class="cu-item arrow" onTap="__e"><view class="content"><text class="text-grey">{{item.name}}</text></view><view class="action"><text class="text-grey text-sm"></text></view></view></block></block></view></block><block a:if="{{TabCur==1}}"><view class="cu-list menu"><view data-event-opts="{{[['tap',[['toList',['time','$0','$1'],[[['times','',index,'months']],[['times','',index,'months']]]]]]]}}" class="cu-item arrow" onTap="__e" a:for="{{times}}" a:for-item="item" a:for-index="index" a:key="index"><view class="content"><text class="text-grey">{{item.months}}</text></view><view class="action"><text class="text-grey text-sm">{{item.count+"篇"}}</text></view></view></view></block><block a:if="{{TabCur==2}}"><view class="cu-list menu"><block a:for="{{tags}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{item.count!==0}}"><view data-event-opts="{{[['tap',[['toList',['tag','$0','$1'],[[['tags','',index,'id']],[['tags','',index,'name']]]]]]]}}" class="cu-item arrow" onTap="__e"><view class="content"><text class="text-grey">{{item.name}}</text></view><view class="action"><text class="text-grey text-sm"></text></view></view></block></block></view></block><bottom-nav vue-id="519f2a76-2" active="{{pageName}}" onVueInit="__l"></bottom-nav></view>